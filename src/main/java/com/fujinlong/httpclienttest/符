function autoCallback(obj){
  if(obj.code ==0){
    $("#auto .contentLeft .imgArea").html('<a target="_blank" href="'+obj.data[0].url+'"><img src="'+obj.data[0].img+'" alt="'+obj.data[0].title+'" /></a>');
    $("#auto .contentLeft .txtArea h3").html('<a target="_blank" href="'+obj.data[0].url+'">'+obj.data[0].title+'</a>');
    var autoLeftDataArr = [];
    for(var i=1;i<7;i++){
      if(obj.data[i].is_locked && obj.data[i].is_locked == "2"){
        autoLeftDataArr.push('<li>');
        for(var n=0; n<obj.data[i].items.length; n++){
          autoLeftDataArr.push('<a class="');
          if(obj.data[i].items[n].ext){
            if(obj.data[i].items[n].ext.style.bold == "1"){ //加粗
              autoLeftDataArr.push('Q-bold ');
            }
            if(obj.data[i].items[n].ext.style.color == "1"){ //红色
              autoLeftDataArr.push('qm_c_1 ');
            }else if(obj.data[i].items[n].ext.style.color == "2"){ //黑色
              autoLeftDataArr.push('qm_c_2 ');
            }else if(obj.data[i].items[n].ext.style.color == "3"){ //蓝色
              autoLeftDataArr.push('qm_c_3 ');
            }
            if(obj.data[i].items[n].ext.style.icon == "1"){ //微博图标
              autoLeftDataArr.push('qm_i_1');
            }else if(obj.data[i].items[n].ext.style.icon == "2"){ //视频图标
              autoLeftDataArr.push('qm_i_2');
            }else if(obj.data[i].items[n].ext.style.icon == "3"){ //图片图标
              autoLeftDataArr.push('qm_i_3');
            }else if(obj.data[i].items[n].ext.style.icon == "4"){ //直播图标
              autoLeftDataArr.push('qm_i_4');
            }else if(obj.data[i].items[n].ext.style.icon == "5"){ //声音图标
              autoLeftDataArr.push('audioico');
            }
          }
          autoLeftDataArr.push('" target="_blank" href="'+obj.data[i].items[n].url+'">'+obj.data[i].items[n].title+'</a>');
          if(obj.data[i].items[n].ext){ //竖线
            if(obj.data[i].items[n].ext.split == "1"){ //竖线
              autoLeftDataArr.push(' | ');
            }else{
              autoLeftDataArr.push(' ');
            }
          }else{
            autoLeftDataArr.push(' ');
          }
        }
        autoLeftDataArr.push('</li>');
      }else{
        autoLeftDataArr.push('<li><a class="');
        if(obj.data[i].ext){
          if(obj.data[i].ext.style.bold == "1"){ //加粗
            autoLeftDataArr.push('Q-bold ');
          }
          if(obj.data[i].ext.style.color == "1"){ //红色
            autoLeftDataArr.push('qm_c_1 ');
          }else if(obj.data[i].ext.style.color == "2"){ //黑色
            autoLeftDataArr.push('qm_c_2 ');
          }else if(obj.data[i].ext.style.color == "3"){ //蓝色
            autoLeftDataArr.push('qm_c_3 ');
          }
          if(obj.data[i].ext.style.icon == "1"){ //微博图标
            autoLeftDataArr.push('qm_i_1');
          }else if(obj.data[i].ext.style.icon == "2"){ //视频图标
            autoLeftDataArr.push('qm_i_2');
          }else if(obj.data[i].ext.style.icon == "3"){ //图片图标
            autoLeftDataArr.push('qm_i_3');
          }else if(obj.data[i].ext.style.icon == "4"){ //直播图标
            autoLeftDataArr.push('qm_i_4');
          }else if(obj.data[i].ext.style.icon == "5"){ //声音图标
            autoLeftDataArr.push('audioico');
          }
        }
        autoLeftDataArr.push('" target="_blank" href="'+obj.data[i].url+'">'+obj.data[i].title+'</a></li>');
      }
    }
    $("#auto .contentLeft ul").html(autoLeftDataArr.join(""));
    
    var autoPriceConArr0 = [];
    for(var i=0;i<6;i++){
    	autoPriceConArr0.push('<li><a target="_blank" href="'+obj.price.data[1][i].url+'">'+obj.price.data[1][i].title+'</a></li>');
    }
    var autoPriceConArr1 = [];
    for(var i=0;i<6;i++){
    	autoPriceConArr1.push('<li><a target="_blank" href="'+obj.price.data[2][i].url+'">'+obj.price.data[2][i].title+'</a></li>');
    }
    var autoPriceConArr2 = [];
    for(var i=0;i<6;i++){
    	autoPriceConArr2.push('<li><a target="_blank" href="'+obj.price.data[3][i].url+'">'+obj.price.data[3][i].title+'</a></li>');
    }
    var autoPriceConArr3 = [];
    for(var i=0;i<6;i++){
    	autoPriceConArr3.push('<li><a target="_blank" href="'+obj.price.data[4][i].url+'">'+obj.price.data[4][i].title+'</a></li>');
    }
    $("#qq-contents6").children().eq(0).find("ul").html(autoPriceConArr0.join(""));
    $("#qq-contents6").children().eq(1).find("ul").html(autoPriceConArr1.join(""));
    $("#qq-contents6").children().eq(2).find("ul").html(autoPriceConArr2.join(""));
    $("#qq-contents6").children().eq(3).find("ul").html(autoPriceConArr3.join(""));
    var autoInterId = obj.price.intere_id;
    $("#qq-tabs6 a").removeClass("selected");
    $("#qq-contents6 div").hide();
    new Tab({tabElement:document.getElementById('qq-tabs6'), contentElement:document.getElementById('qq-contents6'), eventType:'onmouseover', defaultIndex:autoInterId});
  }
}
  
//搜狗灰度
function sogouGray(){
    var uinStr = QQ.Cookie.get("uin") || QQ.Cookie.get("luin") || QQ.Cookie.get("pt2qquin") || QQ.Cookie.get("o_cookie") || "";
    uinStr = parseInt(Number(uinStr.replace("o",""))).toString();
    if(uinStr.charAt(uinStr.length-4) == 1 || uinStr.charAt(uinStr.length-4) == 2 || uinStr.charAt(uinStr.length-4) == 3){ //搜狗灰度1;QQ倒数第四位为1、2、3
        document.getElementById("searchBtn").className = "sougouGray";
        document.getElementById("sougouTxt").style.width = "200px";
        document.getElementById("searchTxt").style.width = "315px";
        document.getElementById("searchBtn").onclick = function(){
            registerZone2({bossZone:'searchbutton_g1',url:''},1);
        }
        document.getElementById("sogouLogoLink").onclick = function(){
            registerZone2({bossZone:'searchlogo_g1',url:''},1);
        }
        document.getElementById("sogouLogoLink").href = "https://www.sogou.com/?pid=sogou-wsse-3f7bcd0b3ea82268-0002";
        document.getElementById("searchForm").setAttribute("action","https://www.sogou.com/sogou?pid=sogou-wsse-3f7bcd0b3ea82268-0002&ie=utf-8&query=");
        return false;
    }else if(uinStr.charAt(uinStr.length-4) == 4 || uinStr.charAt(uinStr.length-4) == 5 || uinStr.charAt(uinStr.length-4) == 6){ //搜狗灰度2;QQ倒数第四位为4、5、6
        document.getElementById("searchBtn").className = "sougouGray2";
        document.getElementById("sougouTxt").style.width = "212px";
        document.getElementById("searchTxt").style.width = "327px";
        document.getElementById("searchBtn").onclick = function(){
            registerZone2({bossZone:'searchbutton_g2',url:''},1);
        }
        document.getElementById("sogouLogoLink").onclick = function(){
            registerZone2({bossZone:'searchlogo_g2',url:''},1);
        }
        document.getElementById("sogouLogoLink").href = "https://www.sogou.com/?pid=sogou-wsse-3f7bcd0b3ea82268-0003";
        document.getElementById("searchForm").setAttribute("action","https://www.sogou.com/sogou?pid=sogou-wsse-3f7bcd0b3ea82268-0003&ie=utf-8&query=");
        return false;
    }else{
        document.getElementById("searchForm").setAttribute("action","https://www.sogou.com/sogou?pid=sogou-wsse-3f7bcd0b3ea82268-0001&ie=utf-8&query=");
    }
}
  
$(function(){
  	//安全网络、社会主义、举报广告轮播
	(function(){
		var _adScrollBtn = $("#adScrollBtn"),
			_adScroll = $(".adScroll"),
			_num = 0,
			_id = [$("#jbBox"),$("#wlaqBox")],
			_gglbsj,
			_lbjg = 3000;//轮播间隔时间
		
		_gglbsj = setInterval(gglb,_lbjg);
		
		function gglb(){//广告轮播函数
			if(_num < 1){
				_id[_num].fadeOut(300,function(){
					_id[_num + 1].fadeIn(300);
					_adScrollBtn.find("span").removeClass("now").eq(_num + 1).addClass("now");
					_num++;
				});
			}else{
				_id[_id.length - 1].fadeOut(300,function(){
					_id[0].fadeIn(300);
					_adScrollBtn.find("span").removeClass("now").eq(0).addClass("now");
					_num = 0;
				});
			};
		};
		
		_adScroll.mouseenter(function(){
			clearInterval(_gglbsj);
		}).mouseleave(function(){
			_gglbsj = setInterval(gglb,_lbjg);
		});	
		
		//点击切换
		_adScrollBtn.find("span").each(function(idx){
			$(this).click(function(){
				clearInterval(_gglbsj);				
				for(var i=0;i<2;i++){
					_id[i].hide();
				};
				_id[idx].fadeIn(300);
				_adScrollBtn.find("span").removeClass("now").eq(idx).addClass("now");
				_num = idx;
			})
		});			
	})();
  	//社会主义核心价值观公益广告轮播
  	/*
    (function(){
        var _jbBox = $("#jbBox"),_gyBox = $("#gyBox"),_gyBoxShow = QQ.localData.get('gyBoxShow');
        if(_gyBoxShow != 0 && _gyBoxShow != 1){
          	 _gyBox.show();
             QQ.localData.set('gyBoxShow','1');
        }else{
            if(_gyBoxShow == 0){
                _jbBox.hide();
                _gyBox.show();
                QQ.localData.set('gyBoxShow','1');
            }else{
                _jbBox.show();
                _gyBox.hide();
                QQ.localData.set('gyBoxShow','0');
            }
        }
    })();
    */
	//曝光异常排查
	defaultPuguang("checkup",2128);
    //订阅板块过滤?字符
    //dyReplace();
    //终端检测
    platformCheck();
    //字体切换
    //fontTurn();
    //字体适配
    //fontCheck();
    //移动设备隐藏自选股浮层
    //stockMobile();
    //返回顶部
    $(window).scroll(function(){
        toTopHide();
        $("#toTop").click(function(){
            window.scrollTo(0,0);
            return false;
        });
    });
    //频道导航更多
    navMore();
    //要闻区数据初始化
    if(window.location.toString().indexOf('page_preview.ninja.webdev.com') == -1){
      //newsInit();
    }
    //搜狗脚本
    sogouInit();
    //大家爱看初始化
    //djakInit();
    //快报、社会、军事初始化
    kuaibaoInit();
    //产品导航扩展
    productNavMore();
    //登录态检查
    login.loginCheck();
    //频道内容初始化

    //房产定向
    //houseIpInit();
  
    //视觉焦点
    picFocusInit();
    //设为首页
    homeSet();
    //二维码
    //erweimaNews();
    //房产产品库smartbox
    houseProduct();
    //教育产品库初始化
    ints(0);
    //汽车产品库
    AutoSiteSearch.init_py_list();
    //移动端左右留白
    if(/AppleWebKit.*Mobile/i.test(navigator.userAgent) || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent))){
        document.getElementsByTagName("body")[0].style.minWidth = "1028px";
    };
});

// NBA比赛
(function () {
		var curTime = serverTime.getTime();
  //var curTime = new Date().getTime();
		var t = window.location.href.split('t=')[1];
		var api = {
			calendar : 'http://sportswebapi.qq.com/tools/calendar',
			    list : 'http://sportswebapi.qq.com/kbs/list'
		}
		// 查询数据
		var todayHasGame = false;
		var today;
		var maxUlWidth;
		$.ajax({
			url: api.calendar,
			dataType: 'jsonp',
			data : {
				competitionId : 100000,
				   webVersion : 1,
						 from : 'kbs_web',
						   _t : new Date().getTime()

			},
			jsonp: 'callback',
			jsonpCallback:'responseData',//jsonp回调函数名称
			success: function(data){
				//today = data[1].today;
				today = t? t : data[1].today;
				var gameData = data[1].data;
				for(var i in gameData) {
					if(gameData[i].date == today) {
						getList();
					}
				}
			},
			error: function(data){

			}
		});
		function getList() {
				$.ajax({
					url: api.list,
					dataType: 'jsonp',
					data : {
						columnId : 100000,
					   startTime : today,
						 endTime : today,
							from : 'qshou_nba',
							  _t : new Date().getTime()

					},
					jsonp: 'callback',
					jsonpCallback:'responseData',//jsonp回调函数名称
					success: function(data){
						list = data.data[today];
						var len = list.length;
						var gameLen = 0;

						if(len > 0) {
							var sHtml = '';
							for(var i = 0; i < len; i ++ ) {
								var time = Format(list[i].startTime);
								// 当前时间 未到 比赛时间
								if(curTime < time) {
									gameLen ++;
									var startTimeLen= list[i].startTime.length;
									sHtml += ''+
									'<li>'+
										'<a target="_blank" href="http://sports.qq.com/kbsweb/game.htm?mid='+ list[i].mid +'">'+
										'<span class="nbaRed"> '+ list[i].startTime.substr((startTimeLen-8), 5) +' </span>'+
											list[i].leftName + 
										'<span class="nbaRed"> VS </span> '+
											list[i].rightName +
										' </a>'+
									'</li>';
								}
							}

							if(gameLen > 1) {
								//多场比赛
								sHtml = sHtml + sHtml;
								maxUlWidth = gameLen * 206;
								$('#nbaGameUl').css('width', maxUlWidth * 2  + 'px');
								$('#nbaGameUl').html(sHtml);
								$('#nbaSaicheng').show();
								$('#nbaSaichengContent').scrollLeft(maxUlWidth);
								
								bindEvt();
							}else if(gameLen == 1){
								// 一场比赛 无滑动
								$('#nbaSaicheng .nbaBtnLeft').hide();
								$('#nbaSaicheng .nbaBtnRight').hide();
								$('#nbaGameUl').html(sHtml);
								$('#nbaSaicheng').show();
							}else{
								// 无比赛 不展示
							}
						}
					},
					error: function(data){

					}
				});
		
		}
		// "2015-10-12 07:00:00" ==> 10/12/2015 07:00:00 => 毫秒数
		function Format(str) {
			var arr = str.split(' ');
			var arrYMD = arr[0].split('-')
			var newTime = arrYMD[1] + '/'+arrYMD[2]+'/'+arrYMD[0] + ' ' + arr[1]
			var time = new Date(newTime).getTime();
			return time;
		}
		function bindEvt() {
			var st;
			var ul = $('#nbaGameUl');
			var div = $('#nbaSaichengContent');
			$('#sportsNba').delegate('.nbaBtnLeft', 'click', function() {
				if(div.is(":animated")) {
					return;
				}
				if(st) {
					clearTimeout(st);
				}
				var sl = div.scrollLeft();
				div.animate(
					{scrollLeft : (sl - 206)},
					'normal',
					'swing',
					function () {
						if(div.scrollLeft() == 0) {
							div.scrollLeft(maxUlWidth)
						}
						st = setTimeout(slide,5000);
					}
				);
			});
			$('#sportsNba').delegate('.nbaBtnRight', 'click', function() {
				if(div.is(":animated")) {
					return;
				}
				if(st) {
					clearTimeout(st);
				}
				var sl = div.scrollLeft();
				div.animate(
					{scrollLeft : (sl + 206)},
					'normal',
					'swing',
					function () {
						if(div.scrollLeft() == (2*maxUlWidth - 206)) {
							div.scrollLeft(maxUlWidth -206)
						}
						st = setTimeout(slide,5000);
					}
				);
			}); 

			
			function slide() {
				var sl = div.scrollLeft();
				div.animate(
					{scrollLeft : (sl + 206)},
					'normal',
					'swing',
					function () {
						if(div.scrollLeft() == (2*maxUlWidth - 206)) {
							div.scrollLeft(maxUlWidth -206)
						}
						
						st = setTimeout(slide,5000);
					}
				);
			}
			st = setTimeout(slide,5000);
		}
	}());


</script>
<!--主脚本 结束-->
