e=2;
if(d)
{
var g=j.document.documentElement;
k=(g.textContent||g.innerText||"").substr(0,30);
}
}
catch(h)
{
doPageError([c,h.message].join(":"),j&&j.location&&j.location.href||c,e);
d=h.message||"exception";
}
QMHistory.recordActionFrameChange();
if(d)
{
callBack.call(i,a,[k]);
errorProcess();
}
}
}
