var dateTime = serverTime, index=0;

var week = dateTime.getDay(), hours = dateTime.getHours(), minutes = dateTime.getMinutes();
/*if((week == 5 && hours >= 17) || week == 6 || (week == 0 && hours < 23)){
    index = 2;
}else */
  
//if(dateTime.getHours() >= 10 && dateTime.getHours() < 22){
/*
if(dateTime.getHours() >= 18 || dateTime.getHours() < 6){
    index = 1;
}
*/
if(week == 5 && hours > 20){
    index = 2;
}else if(week == 5 && hours == 20 && minutes >= 30){
    index = 2;
}else if(hours >= 18 || hours < 6){
    index = 1;	
}

