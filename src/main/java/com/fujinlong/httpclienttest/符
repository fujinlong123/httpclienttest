{
var h=getUrlParams(c||location);
_oCgiPart=(c||"").split("?")[0].split("/"),_sErrFunc=encodeURIComponent(m.replace(/[\r\n\t ]/ig,"").substr(0,50));
if(_oCgiPart.length>0)
{
