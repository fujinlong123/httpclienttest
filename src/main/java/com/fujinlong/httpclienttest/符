{
w=S("draftmailid",s).value;
}
else{
w=S("fmailid",s).value;
}
if(w!=c)
{
if(c.charAt(0)=='@')
{
S("draftmailid",s).value=c;
}
else{
S("fmailid",s).value=c;
}
