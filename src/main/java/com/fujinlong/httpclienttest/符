i-=o.scrollTop;
p=o;
o=o.parentNode;
}
if(q._isSupportsFixedPosition()&&p.style&&getStyle(p,"position")==="fixed")
{
h+=bodyScroll(m,'scrollLeft');
i+=bodyScroll(m,'scrollTop');
}
}
