if(!/(s[016789]bg)|(Rw)/.test(e.className))
{
var i=a?"r":c.getAttribute("rf"),h=c.getAttribute("isendtime"),f="Rr";
if(h)
{
f=h=="0"?"Rc":"Ti";
}
else if(b)
{
f="Ru";
}
else if(i)
{
f=i=="r"?"Rh":"Rz";
}
setClass(e,"cir "+f);
}
return b;
};
