          	}
  			$("#history .bd .contentLeft ul").children().eq(i)[0].innerHTML = milDataTempLeftArr.join("");
  			milDataTempLeftArr = [];
        }
        QQ.LoadScript("http://mini2015.qq.com/ssitojson.htm?games.htm&random="+Math.random(), function(){});
  	}else if(data.key == "games"){//游戏数据
  		$("#games .bd .contentRight .imgArea").html('<a target="_blank" href="'+data.value['pic-text'][2].data[0].url+'"><img src="'+data.value['pic-text'][2].data[0].img+'" alt="'+data.value['pic-text'][2].data[0].title+'"></a>');
  		$("#games .bd .contentRight .txtArea h3").html('<a target="_blank" href="'+data.value['pic-text'][2].data[0].url+'">'+data.value['pic-text'][2].data[0].title+'</a>');
        var gamesDataTempRightArr = [];
        for(var i=8;i<12;i++){
          if(data.value.text[i].data.length != 1){
            gamesDataTempRightArr.push('<li>');
            for(var n=0;n<data.value.text[i].data.length;n++){
                gamesDataTempRightArr.push('<a href="'+data.value.text[i].data[n].url+'" target="_blank" class="'+data.value.text[i].data[0].style.icon+'">'+data.value.text[i].data[n].title+'</a>');
                if(n != data.value.text[i].data.length - 1){
                    gamesDataTempRightArr.push(' ');
                }
            }
            gamesDataTempRightArr.push('</li>');
          }else{
            gamesDataTempRightArr.push('<li><a href="'+data.value.text[i].data[0].url+'" target="_blank" class="'+data.value.text[i].data[0].style.icon+'">'+data.value.text[i].data[0].title+'</a></li>');
          }
        }
        for(var i=13;i<15;i++){
          if(data.value.text[i].data.length != 1){
            gamesDataTempRightArr.push('<li>');
            for(var n=0;n<data.value.text[i].data.length;n++){
                gamesDataTempRightArr.push('<a href="'+data.value.text[i].data[n].url+'" target="_blank" class="'+data.value.text[i].data[0].style.icon+'">'+data.value.text[i].data[n].title+'</a>');
                if(n != data.value.text[i].data.length - 1){
                    gamesDataTempRightArr.push(' ');
                }
            }
            gamesDataTempRightArr.push('</li>');
          }else{
            gamesDataTempRightArr.push('<li><a href="'+data.value.text[i].data[0].url+'" target="_blank" class="'+data.value.text[i].data[0].style.icon+'">'+data.value.text[i].data[0].title+'</a></li>');
          }
        }
        $("#games .bd .contentRight ul")[0].innerHTML = gamesDataTempRightArr.join("");
      	if($("#games").hasClass("contentModBig")){
        	$("#games .bd .contentRight").show();
        }else{
        	$("#games .bd .contentRight").hide();
        }
	QQ.LoadScript("http://mini2015.qq.com/ssitojson.htm?auto.htm&random="+Math.random(), function(){});
  	}else if(data.key == "auto"){//汽车
		var d = data.value.text;
		var len = d.length;
		var str = '';
		if(len && len >= 6) {
			for(var i = 1; i < 6; i ++) {
				str += '' + 
					'<li>'+ 
						'<a  target="_blank" href="'+ d[i].data[0].url +'">'+d[i].data[0].title+'</a> '+	
						(d[i].data[1]? '<a  target="_blank" href="'+ d[i].data[1].url +'">'+d[i].data[1].title+'</a>':'')+				
					'</li>';
			}
		}
		var ul = $('#auto .bd .contentLeft ul');
		var lasrChil = ul.children(':last');
		str += '<li>'+ lasrChil.html() +'</li>';
		ul.html(str);
		QQ.LoadScript("http://mini2015.qq.com/ssitojson.htm?tech.htm&random="+Math.random(), function(){});
		//$('#auto .bd .contentLeft ul').children(":eq(0),:eq(1),:eq(2),:eq(3),:eq(4)").replaceWith(str);
	}else if(data.key == "tech"){//科技数据
		var d = data.value;
		var img = d['pic-text'];
		var text = d.text;
		var len = text.length;
		var str1 = '';
		var str2 = '';
		if(len && len >= 6) {
			for(var i = 0; i < 3; i ++) {
				str1 += '' + 
				'<li bosszone="tech">'+
					'<a target="_blank" href="'+ text[i].data[0].url +'">'+ text[i].data[0].title +'</a>'+
				'</li>';
			}

			for(var j = 3; j < 6; j ++) {
				str2 += '' + 
				'<li bosszone="tech">'+
					'<a target="_blank" href="'+ text[j].data[0].url +'">'+ text[j].data[0].title +'</a>'+
				'</li>';
			}
		}

		
		var contentLeft = $('#tech .bd .contentLeft');
		var ulLeft = contentLeft.find('ul');
		var tarElLeft = ulLeft.find('[bosszone="tech"]');
		tarElLeft.remove();
		ulLeft.prepend(str1);
		contentLeft.find('.imgArea').html(
			'<a target="_blank" href="'+img[1].data[0].url+'">'+
				'<img src="'+ img[1].data[0].img +'" alt="'+ img[1].data[0].title +'">'+
			'</a>'
		
		);
		contentLeft.find('.txtArea').html(
			'<h3><a target="_blank" href="'+ img[1].data[0].url +'">'+img[1].data[0].title+'</a></h3>'
		);
		
		var contentRight = $('#tech .bd .contentRight')
		var ulRight = contentRight.find('ul');;
		var tarElRight = ulRight.find('[bosszone="tech_1"]');
		tarElRight.remove();
		ulRight.prepend(str2);
		contentRight.find('.imgArea').html(
			'<a target="_blank" href="'+img[2].data[0].url+'">'+
				'<img src="'+ img[2].data[0].img +'" alt="'+ img[2].data[0].title +'">'+
			'</a>'
		)
		contentRight.find('.txtArea').html(
			'<h3><a target="_blank" href="'+ img[2].data[0].url +'">'+img[2].data[0].title+'</a></h3>'
		);



		//var lasrChil = ul.children(':last');
		//str += '<li>'+ lasrChil.html() +'</li>';
		//ul.html(str);
		//$('#auto .bd .contentLeft ul').children(":eq(0),:eq(1),:eq(2),:eq(3),:eq(4)").replaceWith(str);
	}
}

