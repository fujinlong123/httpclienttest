function decreaseFolderUnread(c,a,b)
{
var d,e=c.split(';');
for(var f=e.length-1;f>=0;f--)
{
if(d=optFolderUnread(0,e[f]))
{
optFolderUnread(1,e[f],d-1,a,b);
}
}
}
