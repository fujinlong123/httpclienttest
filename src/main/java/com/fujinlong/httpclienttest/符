function _trimUrlSearchParam(a)
{
return a&&a.substr&&("?"+(["&",a.substr(1),"&"].join("").replace(/&sid=.*?&/ig,"&").replace(/&loc=.*?&/ig,"&").replace(/&newwin=true/ig,"&").slice(1,-1)));
}
function goNewWin(d,b,a,c)
{
var f="",h="",i="";
if(typeof (d)=="object")
{
f=d.pathname;
h=d.search;
}
else{
var e=d.indexOf("?");
f=d.substring(0,e).replace(/^(https?:)?\/\/[^\/]+/,'');
h=d.substr(e);
}
